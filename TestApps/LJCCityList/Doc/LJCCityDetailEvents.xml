<!-- LJCTable.xml -->
<items>
  <!-- Event Handlers -->
  <CancelClick>
    <summary>Close the dialog without updating the data.</summary>
    <param name="event">The Target event.</param>
  </CancelClick>
  <CommitClick>
    <summary>Update data and close dialog.</summary>
    <param name="event">The Target event.</param>
  </CommitClick>
  <!-- Other Methods -->
  <ValidFormValues>
    <summary>Checks the form values.</summary>
    <param name="city">The City data object.</param>
    <returns>true if successful; otherwise false.</returns>
  </ValidFormValues>
  <DataRequest>
    <summary>Call the web service.</summary>
    <param name="cityRequest">The city request object.</param>
  </DataRequest>
</items>
